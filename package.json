{
  "name": "cocreate-backend",
  "version": "1.0.0",
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  },
  "description": "/",
  "scripts": {
    "db:init": "npx prisma migrate dev && npx prisma db seed",
    "build": "npx nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "tsc": "tsc",
    "lint": "eslint ./src/*",
    "lint:fix": "eslint ./src/* --fix",
    "test": "jest --testPathPattern=dist/ --rootDir=./",
    "heroku-postbuild": "npm install --legacy-peer-deps && npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdp-cocreate/cocreate-backend.git"
  },
  "keywords": [
    "nest",
    "api"
  ],
  "author": "Edgar Cresson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdp-cocreate/cocreate-backend/issues"
  },
  "homepage": "https://github.com/mdp-cocreate/cocreate-backend#readme",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@prisma/client": "^4.11.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "slugify": "^1.6.6",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.14.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
